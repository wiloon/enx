# ENX Data Service Environment Configuration
# Copy this file to .env and modify for your local environment
# .env file should NOT be committed to git

# Database Path (sensitive, node-specific)
DB_PATH=/var/lib/enx-api/enx.db

# Node Configuration (node-specific)
NODE_ID=desktop-001
GRPC_PORT=50051
HTTP_PORT=8090

# Peer Nodes (comma-separated addresses)
# Names will be auto-generated as peer-1, peer-2, etc.
PEERS=192.168.1.10:50051,192.168.1.20:50051
